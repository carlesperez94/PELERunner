{
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "pulledAtom": {
            "type": "string",
            "required": true
        },
        "fixedAtom": {
            "type": "string",
            "required": true
        },
        "beadVelocity": {
            "type": "number",
            "default": 0.01
        },
        "pulledAtomSpringConstant": {
            "type": "number",
            "default": 3.5
        },
        "fixedAtomSpringConstant": {
            "type": "number",
            "default": 300
        },
        "equilibriumLength": {
            "type": "number",
            "default": 0
        },
        "forceVsLengthDataPath": {
            "type": "string",
            "default": ""
        },
        "frequency": {
            "type": "integer",
            "minimum": 0,
            "default": 1
        },
        "removeBulk":{
        	"type": "boolean",
        	"default": false
    	},
        "updatePullingDirectionInEachStep":{
        	"type": "boolean",
        	"default": false
    	},
        "restartDataPath": {
            "type": "string",
            "default": ""
        },
        "HydrogenBondsPerturbation": @HidrogenBondsPerturbation@
    }
}